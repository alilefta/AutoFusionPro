<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:wpfUi="http://schemas.lepo.co/wpfui/2022/xaml">



    <!-- ==================== Base ==================== -->
    <SolidColorBrush x:Key="Window.BackgroundBrush"
                     Color="{StaticResource Neutral50}" />
    <SolidColorBrush x:Key="Content.BackgroundBrush"
                     Color="{StaticResource Neutral0}" />
    
    
    
    <!-- Card/Dialog Bodies -->
    <SolidColorBrush x:Key="Sidebar.BackgroundBrush"
                     Color="{StaticResource Neutral200}" />
    <SolidColorBrush x:Key="Header.BackgroundBrush"
                     Color="{StaticResource Neutral150}" />
    <SolidColorBrush x:Key="Toolbar.BackgroundBrush"
                     Color="{StaticResource Neutral150}" />
    <SolidColorBrush x:Key="StatusBar.BackgroundBrush"
                     Color="{StaticResource Neutral150}" />
    
    <SolidColorBrush x:Key="Dialog.BackgroundBrush"
                     Color="{StaticResource Neutral0}" />
    <SolidColorBrush x:Key="Dialog.Title.ForegroundBrush"
                     Color="{StaticResource Neutral750}" />
    <SolidColorBrush x:Key="Dialog.Subtitle.ForegroundBrush"
                     Color="{StaticResource Neutral650}" />
    
    <SolidColorBrush x:Key="Tooltip.BackgroundBrush"
                     Color="{StaticResource Neutral750}" />
    <!-- Dark Tooltip -->
    <SolidColorBrush x:Key="Tooltip.ForegroundBrush"
                     Color="{StaticResource Neutral100}" />

    <!-- ==================== Text ==================== -->
    <SolidColorBrush x:Key="Text.PrimaryBrush"
                     Color="{StaticResource Neutral750}" />
    <!-- Main Body -->
    <SolidColorBrush x:Key="Text.SecondaryBrush"
                     Color="{StaticResource Neutral600}" />
    <!-- Subtle -->
    <SolidColorBrush x:Key="Text.HeaderBrush"
                     Color="{StaticResource Neutral900}" />
    <!-- Use darker Neutral for headers -->
    <SolidColorBrush x:Key="Text.DisabledBrush"
                     Color="{StaticResource Neutral400}" />
    <SolidColorBrush x:Key="Text.LinkBrush"
                     Color="{StaticResource Link500}" />
    <SolidColorBrush x:Key="Text.AccentBrush"
                     Color="{StaticResource Primary700}" />
    <!-- Text on accent bg -->
    <SolidColorBrush x:Key="Text.ErrorBrush"
                     Color="{StaticResource Error500}" />
    <SolidColorBrush x:Key="Text.SuccessBrush"
                     Color="{StaticResource Success500}" />
    <SolidColorBrush x:Key="Text.WarningBrush"
                     Color="{StaticResource Warning500}" />
    <SolidColorBrush x:Key="Text.InfoBrush"
                     Color="{StaticResource Info500}" />
    <SolidColorBrush x:Key="Text.OnPrimaryBrush"
                     Color="{StaticResource Neutral0}" />
    <!-- Text on Primary Button -->
    <SolidColorBrush x:Key="Text.OnAccentBrush"
                     Color="{StaticResource Neutral0}" />
    <!-- Text on Accent Button/BG -->

    <!-- ==================== Borders & Dividers ==================== -->
    <SolidColorBrush x:Key="Border.PrimaryBrush"
                     Color="{StaticResource Neutral350}" />
    <!-- Default Borders -->
    <SolidColorBrush x:Key="Border.SecondaryBrush"
                     Color="{StaticResource Neutral300}" />
    <!-- Dividers -->
    <SolidColorBrush x:Key="Border.FocusBrush"
                     Color="{StaticResource Primary700}" />
    <!-- Input Focus -->
    <SolidColorBrush x:Key="Border.ErrorBrush"
                     Color="{StaticResource Error500}" />
    <SolidColorBrush x:Key="Border.AccentBrush"
                     Color="{StaticResource Primary500}" />
    <!-- e.g., Selected Tab -->

    <!-- ==================== Controls General ==================== -->
    <SolidColorBrush x:Key="Control.Background.HoverBrush"
                     Color="{StaticResource Neutral150}" />
    <SolidColorBrush x:Key="Control.Background.PressedBrush"
                     Color="{StaticResource Neutral250}" />
    <SolidColorBrush x:Key="Control.Background.SelectedBrush"
                     Color="{StaticResource Primary100}" />
    <SolidColorBrush x:Key="Control.Background.DisabledBrush"
                     Color="{StaticResource Neutral100}" />
    

    <!-- ==================== Toogle Button ==================== -->
    <SolidColorBrush x:Key="ToggleButton.BackgroundBrush"
                     Color="{StaticResource Neutral400}" />

    <SolidColorBrush x:Key="ToggleButton.Thumb.BackgroundBrush"
                     Color="{StaticResource Neutral700}" />

    <!-- ==================== Tab Controls ==================== -->

    <!-- Normal -->
    <SolidColorBrush x:Key="TabView.BackgroundBrush"
                     Color="{StaticResource Neutral100}" />
    <SolidColorBrush x:Key="TabView.BorderBrush"
                     Color="{StaticResource CardStrokeColorDefault}" />
    
    
    <SolidColorBrush x:Key="TabViewItem.ForegroundBrush"
                     Color="{StaticResource Neutral600}" />

    <SolidColorBrush x:Key="TabViewItem.Header.BackgroundBrush"
                     Color="{StaticResource SubtleFillColorTransparent}" />


    <!-- Selected -->
    <SolidColorBrush x:Key="TabViewItem.Selected.ForegroundBrush"
                     Color="{StaticResource Primary500}" />
    <SolidColorBrush x:Key="TabViewItem.Selected.BackgroundBrush"
                     Color="{StaticResource Neutral150}" />

    <SolidColorBrush x:Key="TabViewItem.Selected.BorderBrush"
                     Color="{StaticResource Primary500}" />

    <SolidColorBrush x:Key="TabViewItem.Header.Selected.ForegroundBrush"
                     Color="{StaticResource Primary600}" />

    <!-- Hover -->
    <SolidColorBrush x:Key="TabViewItem.ForegroundBrush.Hover"
                     Color="{StaticResource Neutral600}" />
    <SolidColorBrush x:Key="TabViewItem.BackgroundBrush.Hover"
                     Color="{StaticResource Neutral300}" />


    <!-- ==================== Loading Indicator ==================== -->
    <SolidColorBrush x:Key="LoadingIndicator.BackgroundBrush"
                     Color="{StaticResource Primary600}" />

    <!-- ==================== Button: Default (Light Appearance) ==================== -->
